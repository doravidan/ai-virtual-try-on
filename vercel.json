{
  "version": 2,
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/index.py"
    },
    {
      "source": "/extract-image",
      "destination": "/api/index.py"
    },
    {
      "source": "/generate",
      "destination": "/api/index.py"
    },
    {
      "source": "/favicon.ico",
      "destination": "/api/index.py"
    },
    {
      "source": "/",
      "destination": "/static/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/static/$1"
    }
  ]
}
